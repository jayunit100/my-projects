{
  "apiVersion": "V2",
  "name": "kubectl-multi-cluster",
  "count": 1,
  "haproxy": {
    "stats": {
      "bindPort": 9090
    },
    "frontends": [{
      "bindPort": 6445,
      "protocol": "HTTP",
      "linkBackend": {
        "defaultBackend": "kubernetes-prod"
      },
      "bindPort":  6444,
      "protocol": "HTTP",
      "linkBackend": {
        "defaultBackend": "kubernetes-dev"
      }
    }],
    "backends": [{
      "name": "kubernetes-prod",
      "protocol": "HTTP",
      "services": [{
        "endpoint": {
          "type": "ADDRESS",
          "address": "apiserver.prodkubernetes.l4lb.thisdcos.directory",
          "port": 6443
        }
      }],
      "name": "kubernetes-dev",
      "protocol": "HTTP",
      "services": [{
        "endpoint": {
          "type": "ADDRESS",
          "address": "apiserver.devkubernetes.l4lb.thisdcos.directory",
          "port": 6443
        }
      }]
    }]
  }
}
